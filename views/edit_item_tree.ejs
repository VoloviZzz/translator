<link rel="stylesheet" type="text/css" href="../css/profile.css">
<div class="login">
  <div class="login-body-news">
    <h3 class="login-heading">Редактировать локацию</h3>
    <hr>
    <div class="login-form">
      <div class="form-group">
        <ul class="list-inline">
          <label for="title">Название</label>
          <input id="name" class="form-control" type="text">
        </ul>
      </div>
      <div class="form-group">
        <ul class="list-inline">
          <label for="title">ip</label>
          <input id="ip" class="form-control" type="text" placeholder="255.255.255:0000">
        </ul>
      </div>
      <div class="form-group">
        <ul class="list-inline">
          <label for="photo" id="uploadFiles_label">Герб</label>
          <input id="uploadFiles" class="form-control" type="file" style="display:none">
        </ul>
      </div>
      <div class="form-group">
        <ul class="list-inline">
          <label for="title">Тип модуля</label><br>
          <select id="type" >
            <option value="0">Нет</option>
            <option value="1">Стартовый</option>
            <option value="2">Полный</option>
          </select>
        </ul>
      </div>
      <div class="form-group">
        <ul class="list-inline">
          <label for="title">Статус Инвентаризации</label><br>
          <select id="status" >
            <option value="0">Не проведена</option>
            <option value="1">Выполняется</option>
            <option value="2">Завершина</option>
          </select>
        </ul>
      </div>
      <div class="form-group">
        <div class="btn btn-success btn-block" id="edit_item_tree">Сохранить изменения</div>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript" src="../js/uploader0.js"></script>
<script type="text/javascript" src="../js/edit_item_tree.js"></script>
