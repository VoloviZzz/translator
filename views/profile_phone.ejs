<link rel="stylesheet" type="text/css" href="../css/profile.css">
<div class="login">
  <div class="login-body">
    <h3 class="login-heading">Учётная запись</h3>
    <hr>
    <div class="login-form">
      <div class="form-group">
        <ul class="list-inline">
          <li>
            <small>Персональная информация</small>
          </li>
        </ul>
      </div>
      <div class="form-group">
        <ul class="list-inline">
          <li>
            <big id="fio"></big>
          </li>
        </ul>
      </div>
      <div class="form-group">
        <ul class="list-inline">
          <li>
            <big id="b_date"></big>
          </li>
        </ul>
      </div>
      <div class="form-group">
        <a href="/edit_profile_phone/<%=phone%>" class="btn btn-primary btn-block" >Редактировать данные</a>
      </div>
      <hr>
      <div class="form-group">
        <ul class="list-inline">
          <li>
            <small>Контактные данные</small>
          </li>
        </ul>
      </div>
      <div class="form-group">
        <ul class="list-inline">
          <li>
            <big id="phone"><%=phone%></big>
          </li>
        </ul>
      </div>
      <div class="form-group">
        <ul class="list-inline">
          <li>
            <big id="email"></big>
          </li>
        </ul>
      </div>
      <hr>
      <div class="form-group">
        <ul class="list-inline">
          <li>
            <small>Настройка безопасности</small>
          </li>
        </ul>
      </div>
      <div class="form-group">
        <a href="/password" class="btn btn-success btn-block">Сменить пароль</a>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript" src="../js/profile.js"></script>
